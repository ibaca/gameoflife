<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.1//EN" "http://gwtproject.org/doctype/2.8.1/gwt-module.dtd">
<module rename-to="app">
    <inherits name="com.google.gwt.user.User"/>
    <inherits name="com.intendia.rxgwt2.RxUser"/>
    <inherits name="com.intendia.rxgwt2.RxElemento"/>
    <inherits name="elemental2.dom.Dom"/>
    <entry-point class="gameoflife.Simulator"/>
    <add-linker name="direct_install"/>
    <set-configuration-property name="CssResource.enableGss" value="true"/>
    <collapse-all-properties/>
    <source path="" excludes="public/*"/>
    <public path="public"/>
</module>
